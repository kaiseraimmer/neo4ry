{
  "version": 2,
  "builds": [
    {
      "src": "pages/api/rola.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/api/peso.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/rola",
      "dest": "/pages/api/rola.js"
    },
    {
      "src": "/api/tamanho",
      "dest": "/pages/api/peso.js"
    }
  ]
}
